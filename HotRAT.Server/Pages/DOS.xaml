<Page x:Class="HotRAT.Server.Pages.DOS"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:HotRAT.Server.Pages"
      xmlns:ui1="http://schemas.lepo.co/wpfui/2022/xaml"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="DOS">

    <Grid>
        <TextBox
                x:Name="MainCmd"
                AcceptsReturn="True"
                Foreground="White"
                PreviewKeyDown="MainCmd_PreviewKeyDown"
                PreviewMouseDown="MainCmd_PreviewMouseDown"
                TextWrapping="Wrap"
                VerticalScrollBarVisibility="Hidden" BorderBrush="{x:Null}" FontFamily="Cascadia Mono" FontSize="16" MouseMove="MainCmd_MouseMove" />
    </Grid>
</Page>
